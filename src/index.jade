extends base.jade

block content

  #tech.container
    .left-container
      h3 projects
      ul.projects
        li
          a(href="http://carrafa.github.io/dungeon-land/") dungeon land
          summary a pixel art dungeon explorer
        li
          a(href="http://railstraunt.herokuapp.com/") railstraunt
          summary a restaurant menu and order management system
        li
          a(href="#") typedriver
          summary a multi-player online racing game
        li
          a(href="http://joshua.carrafa.com/brushy") brushy
          summary brush your teeth!
        li
          a(href="http://github.com/carrafa/batmanify/").batmanify batmanify
          summary a jQuery plugin from 1966
    .right-container
      h3.technologies-header technologies
      ul.technologies
        li javascript
        li css
        li html
        li jade
        li express
        li angular
        li mongo
        li rails
        li postresql
    .ocean-surface
      .ship-container
        .tip-of-ship
        .top-of-ship 
          h3
        .bow
        .ship
        .anchor-container
          i.fa.fa-anchor
  
  #music.container
    .acquarium
      .fish-container
        .body
          .eye
        .tail
    .left-container
      h3 music
        ul.projects
          li
            a(href="#" id="poc") plates of cake
          li
            a(href="#" id="om") old monk
          li
            a(href="#" id="nexf") new england axe factory
    .right-container
      .bandcamp-widget
        .om
          <iframe style="border: 0; width: 350px; height: 470px;" src="https://bandcamp.com/EmbeddedPlayer/album=3360441649/size=large/bgcol=ffffff/linkcol=0687f5/tracklist=false/transparent=true/" seamless><a href="http://eeniemeenie.bandcamp.com/album/posing-as-love">Posing as Love by Old Monk</a></iframe>
        .poc
          <iframe style="border: 0; width: 350px; height: 470px;" src="https://bandcamp.com/EmbeddedPlayer/album=151486288/size=large/bgcol=ffffff/linkcol=0687f5/tracklist=false/transparent=true/" seamless><a href="http://platesofcake.bandcamp.com/album/becoming-double">Becoming Double by Plates of Cake</a></iframe>
        .nexf
          <iframe style="border: 0; width: 350px; height: 470px;" src="https://bandcamp.com/EmbeddedPlayer/album=1289132280/size=large/bgcol=ffffff/linkcol=0687f5/tracklist=false/transparent=true/" seamless><a href="http://newenglandaxefactory.bandcamp.com/album/bx-swift">Bx Swift by New England Axe Factory</a></iframe>
      
  
  #design.container
    .left-container
      h3 design
      ul.projects
        li
          a(href="https://vimeo.com/user8413185") music videos
        li
          a(href="musichistoryingifs.com") music history in gifs
        li
          a(href="#") album covers
    .right-container
      
  #ocean.container
